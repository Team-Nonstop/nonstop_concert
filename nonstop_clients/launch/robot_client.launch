<launch>
    <arg name="robot_name" value="samsung"/>
    <arg name="robot_type" value="turtlebot"/>
    <arg name="concert_name" value="nonstop_concert"/>
    <arg name="rapp_lists" value="nonstop_rapps/nonstop.rapps"/>
    <arg name="gateway_watch_loop_period" value="1"/>
<group ns="$(arg robot_name)">
  <!-- TODO - MAKE NODE-->
  <node pkg="tf" type="static_transform_publisher" name="base_ref" args="0 0 0 0 0 0 /map odom 100" />
  
  <!--  ****************** 3d sensor ******************  -->
  <include file="$(find turtlebot_bringup)/launch/3dsensor.launch">
    <arg name="rgb_processing" value="true"/>
    <arg name="scan_processing" value="true"/>
    <arg name="depth_processing" value="false"/>
  </include>

  <include file="$(find turtlebot_nonstop_bringup)/launch/minimal_with_appmanager.launch">
    <arg name="robot_name" value="$(arg robot_name)"/>
    <arg name="robot_type" value="$(arg robot_type)"/>
    <arg name="concert_name" value="$(arg concert_name)"/>
    <arg name="rapp_lists" value="$(arg rapp_lists)"/>
    <arg name="gateway_watch_loop_period" value="1"/>
    <arg name="attached_marker" value="1"/>
    <arg name="safety_controller" value="1"/>
  </include>
</group>
</launch>
